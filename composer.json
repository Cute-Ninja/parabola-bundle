{
  "name": "cuteninja/parabola-bundle",
  "type": "symfony-bundle",
  "description": "Common code handling API for Cute-Ninja projects (ResponseBuilder, Testing, ...)",
  "homepage": "https://github.com/Cute-Ninja/parabola-bundle",
  "keywords": [
    "api",
    "testing"
  ],
  "authors": [
    {
      "name": "Ghriim"
    }
  ],
  "require": {
    "php": ">=5.3.9",
    "symfony/symfony": "2.7.*",
    "doctrine/orm": "^2.4.8",
    "doctrine/doctrine-bundle": "~1.4",
    "symfony/assetic-bundle": "~2.3",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/monolog-bundle": "~2.4",
    "sensio/distribution-bundle": "4.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "incenteev/composer-parameter-handler": "~2.0",
    "jms/serializer-bundle": "1.0.*",
    "friendsofsymfony/rest-bundle": "1.7.*",
    "nelmio/api-doc-bundle": "2.9.*",
    "knplabs/knp-paginator-bundle": "2.5.*",

    "cuteninja/memoria-bundle": "dev-master"
  },
  "require-dev": {
    "doctrine/doctrine-bundle": "~1.3",
    "symfony/yaml": "~2.3",
    "symfony/validator": "~2.3",
    "phpunit/phpunit": "4.8.*",
    "behat/symfony2-extension": "2.1.*"
  },
  "autoload": {
    "psr-4": { "CuteNinja\\ParabolaBundle\\": "" }
  },
  "license": "proprietary"
